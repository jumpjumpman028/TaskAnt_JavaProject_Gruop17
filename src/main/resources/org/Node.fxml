<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="root" alignment="TOP_CENTER" prefHeight="87.0" prefWidth="120.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.Node.NodeController">
   <ImageView fitHeight="86.0" fitWidth="130.0" pickOnBounds="true" preserveRatio="true" StackPane.alignment="CENTER">
      <image>
         <Image url="@../images/UIboard.png" />
      </image>
   </ImageView>
    <ImageView fx:id="icon" fitHeight="24.0" fitWidth="129.0" nodeOrientation="INHERIT" translateY="-10.0" StackPane.alignment="TOP_CENTER">
      <image>
         <Image url="@../images/IRONYTITLE.png" />
      </image>
      <cursor>
         <Cursor fx:constant="DEFAULT" />
      </cursor>
   </ImageView>
   <ImageView fx:id="exclamationIcon" fitHeight="53.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true" translateX="20.0" translateY="-20.0" StackPane.alignment="TOP_RIGHT">
      <image>
         <Image url="@../images/Exclamation_Gray.png" />
      </image>
   </ImageView>
   <ImageView fx:id="exclamationIconChange" fitHeight="60.0" fitWidth="47.0" pickOnBounds="true" preserveRatio="true" translateX="20.0" translateY="-20.0" visible="false" StackPane.alignment="TOP_RIGHT">
      <image>
         <Image url="@../images/Exclamation_Yellow.png" />
      </image>
   </ImageView>
    <Text fx:id="label" style="-fx-font-size: 16px;" text="節點名稱" translateY="-10.0" wrappingWidth="73.0" StackPane.alignment="TOP_CENTER">
      <font>
         <Font name="System Bold Italic" size="12.0" />
      </font></Text>
   <Label fx:id="startTimeLabel" alignment="TOP_LEFT" prefHeight="31.0" prefWidth="113.0" text="Label" translateY="15.0" wrapText="true">
      <font>
         <Font name="Georgia Bold Italic" size="12.0" />
      </font>
   </Label>
   <Label fx:id="endTimeLabel" alignment="TOP_LEFT" prefHeight="31.0" prefWidth="113.0" text="Label" translateY="45.0" wrapText="true">
      <font>
         <Font name="Georgia Bold Italic" size="12.0" />
      </font>
   </Label>
</StackPane>
